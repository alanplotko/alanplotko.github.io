    "scripts": {
        "dev": "gulp dev",
        "staging": "gulp staging",
        "prod": "gulp prod",
        "docker": "yarn docker-dev",
        "docker-dev": "docker compose -f ./docker/development.yml --env-file ./docker/config/.env.local up",
        "watch": "yarn watch-dev",
        "watch-dev": "gulp watch",
        "serve": "yarn dev && yarn serve-dev",
        "serve-dev": "bundle exec jekyll serve --skip-initial-build --no-watch --config './_config/common.yml,./_config/development.yml'",
        "lighthouse-desktop": "npx unlighthouse --site http://localhost:4000 --debug --no-cache --throttle --samples 3 --enable-javascript --desktop",
        "lighthouse-mobile": "npx unlighthouse --site http://localhost:4000 --debug --no-cache --throttle --samples 3 --enable-javascript --mobile"
    },
