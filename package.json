{
    "name": "alanplotko.github.io",
    "version": "1.0.0",
    "main": "index.js",
    "repository": "git@github.com:alanplotko/alanplotko.github.io.git",
    "author": "Alan Plotko <5394528+alanplotko@users.noreply.github.com>",
    "license": "MIT",
    "scripts": {
        "yaml": "yarn yaml-dev & yarn yaml-staging & yarn yaml-prod",
        "yaml-dev": "./node_modules/.bin/gulp yaml",
        "yaml-staging": "JEKYLL_ENV=staging ./node_modules/.bin/gulp yaml",
        "yaml-prod": "JEKYLL_ENV=production ./node_modules/.bin/gulp yaml",
        "staticman": "./node_modules/.bin/gulp staticman",
        "staticman-prod": "JEKYLL_ENV=production ./node_modules/.bin/gulp staticman",
        "dev": "./node_modules/.bin/gulp build",
        "staging": "JEKYLL_ENV=staging ./node_modules/.bin/gulp build",
        "prod": "JEKYLL_ENV=production ./node_modules/.bin/gulp build",
        "watch": "./node_modules/.bin/gulp watch",
        "watch-staging": "JEKYLL_ENV=staging ./node_modules/.bin/gulp watch",
        "serve": "./node_modules/.bin/gulp serve",
        "serve-staging": "JEKYLL_ENV=staging ./node_modules/.bin/gulp serve",
        "serve-prod": "JEKYLL_ENV=production ./node_modules/.bin/gulp serve",
        "test": "yarn lighthouse-all",
        "lighthouse-all": "yarn lighthouse-desktop & yarn lighthouse-mobile",
        "lighthouse-desktop": "./node_modules/.bin/lighthouse http://localhost:4000 --preset=desktop --quiet --chrome-flags='--headless' --view --output-path ./reports/lighthouse-desktop.html",
        "lighthouse-mobile": "./node_modules/.bin/lighthouse http://localhost:4000 --quiet --chrome-flags='--headless' --view --output-path ./reports/lighthouse-mobile.html"
    },
    "devDependencies": {
        "@alexlafroscia/yaml-merge": "^4.1.0",
        "bootstrap": "^5.1.3",
        "child_process": "^1.0.2",
        "chrome-launcher": "^0.15.0",
        "del": "^6.0.0",
        "fs": "^0.0.1-security",
        "glob": "^7.2.0",
        "gulp": "^4.0.2",
        "gulp-htmlmin": "^5.0.1",
        "gulp-jsonmin": "^1.2.0",
        "gulp-purgecss": "^4.1.3",
        "gulp-sass": "^5.0.0",
        "gulp-uglify": "^3.0.2",
        "lighthouse": "^9.2.0",
        "merge-stream": "^2.0.0",
        "normalize.css": "^8.0.1",
        "path": "^0.12.7",
        "sass": "^1.45.1",
        "url": "^0.11.0",
        "workbox-build": "^6.4.2",
        "yargs": "^17.3.1"
    },
    "resolutions": {
        "glob-parent": "^5.1.2"
    },
    "dependencies": {}
}
